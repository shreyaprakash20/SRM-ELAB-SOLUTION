Grade=str(input())
if Grade=='A+' or Grade=='A':
  	print("4.0")
elif Grade=='A-':
  	print("3.7")
elif Grade=='B+':
  	print("3.3")
elif Grade=='B':
  	print("3.0")
elif Grade=='B-':
  	print("2.7")
elif Grade=='C+':
  	print("2.3")
elif Grade=='C':
  	print("2.0")
elif Grade=='C-':
  	print("1.7")
elif Grade=='D+':
  	print("1.3")
elif Grade=='D':
  	print("1.0")
elif Grade=='F':
  	print("0")
    
    
   
